{
  "name": "codeassist",
  "version": "0.0.1",
  "description": "Codeassist Devtools debugging tool",
  "main": "index.js",
  "module": "dist/codeassist.mjs",
  "unpkg": "dist/codeassist.iife.js",
  "jsdelivr": "dist/codeassist.iife.js",
  "types": "dist/codeassist.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/codeassist.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/codeassist.prod.cjs",
          "development": "./dist/codeassist.mjs",
          "default": "./dist/codeassist.mjs"
        },
        "require": {
          "production": "./dist/codeassist.prod.cjs",
          "development": "./dist/codeassist.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/codeassist.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "Julien-Martin13@gmail.com"
  },
  "funding": "https://github.com/sponsors/Julien-Martin",
  "scripts": {
    "build": "rimraf dist && rollup -c ../../rollup.config.js --environment TARGET:codeassist",
    "build:dts": "api-extractor run --local --verbose && tail -n +3 ./src/globalExtensions.ts >> dist/codeassist.d.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l codeassist -r 1",
    "test:dts": "tsc -p ./test-dts/tsconfig.json",
    "test": "yarn run build && yarn run build:dts && yarn test:dts"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.cjs",
    "dist/codeassist.d.ts",
    "index.js",
    "index.cjs",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "vue",
    "codeassist",
    "composition",
    "api",
    "setup",
    "typed",
    "typescript",
    "ts",
    "type",
    "safe",
    "devtools",
    "nuxt"
  ],
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "7.18.19",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/server-renderer": "^3.2.22",
    "@vue/test-utils": "^2.0.0-rc.16",
    "vue": "^3.2.22",
    "vue2": "npm:vue@2"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.0-beta.20.1",
    "vue-demi": "*"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.0",
    "typescript": "^4.4.4",
    "vue": "^2.6.14 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@vue/composition-api": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-Martin/codeassist.git"
  },
  "bugs": {
    "url": "https://github.com/Julien-Martin/codeassist/issues"
  },
  "homepage": "https://github.com/Julien-Martin/codeassist#readme"
}