{
  "name": "@codeassist/devtools",
  "version": "0.0.1",
  "main": "dist/codeassist-devtools.cjs.js",
  "module": "dist/codeassist-devtools.esm-bundler.js",
  "unpkg": "dist/codeassist-devtools.global.js",
  "jsdelivr": "dist/codeassist-devtools.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/codeassist-devtools.cjs.js",
      "browser": "./dist/codeassist-devtools.esm-browser.js",
      "import": "./dist/codeassist-devtools.esm-bundler.js",
      "module": "./dist/codeassist-devtools.esm-bundler.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && rollup -c ../../rollup.config.js --environment TARGET:devtools"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.0-beta.14"
  },
  "peerDependencies": {
    "vue": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "typescript": "^4.1.3",
    "vue": "^3.1.0"
  }
}
